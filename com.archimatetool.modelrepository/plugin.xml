   <extension
         point="org.eclipse.ui.views">
      <view
            class="com.archimatetool.modelrepository.views.repositories.ModelRepositoryView"
            icon="img/plugin.png"
            id="com.archimatetool.modelrepository.modelRepositoryView"
            name="Collaboration Workspace"
            restorable="true">
      </view>
      <view
            class="com.archimatetool.modelrepository.views.history.HistoryView"
            icon="img/history_view.png"
            id="com.archimatetool.modelrepository.historyView"
            name="Change History"
            restorable="true">
      </view>
      <view
            class="com.archimatetool.modelrepository.views.branches.BranchesView"
            icon="img/branches_obj.png"
            id="com.archimatetool.modelrepository.branchesView"
            name="Branches"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="*">
         <view
               id="com.archimatetool.modelrepository.modelRepositoryView"
               minimized="false"
               relationship="stack"
               relative="com.archimatetool.editor.navigatorView">
         </view>
         <view
               id="com.archimatetool.modelrepository.historyView"
               minimized="false"
               relationship="stack"
               relative="com.archimatetool.editor.propertiesView">
         </view>
         <view
               id="com.archimatetool.modelrepository.branchesView"
               minimized="false"
               relationship="stack"
               relative="com.archimatetool.modelrepository.historyView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
