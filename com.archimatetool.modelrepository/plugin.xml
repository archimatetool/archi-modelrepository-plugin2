<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="com.archimatetool.modelrepository.category"
            name="coArchi 2">
      </category>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.ShowRepositoryViewHandler"
            id="com.archimatetool.modelrepository.command.showRepositoryView"
            name="Toggle Collaboration Workspace">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.ShowHistoryViewHandler"
            id="com.archimatetool.modelrepository.command.showHistoryView"
            name="Toggle Change History">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.ShowBranchesViewHandler"
            id="com.archimatetool.modelrepository.command.showBranchesView"
            name="Toggle Branches">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.ShowInRepositoryViewHandler"
            id="com.archimatetool.modelrepository.command.showInRepositoryView"
            name="Show in Collaboration Workspace">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.ShowInHistoryViewHandler"
            id="com.archimatetool.modelrepository.command.showInHistoryView"
            name="Show in Change History">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.ShowInBranchesViewHandler"
            id="com.archimatetool.modelrepository.command.showInBranchesView"
            name="Show in Branches">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.CreateRepoFromModelHandler"
            id="com.archimatetool.modelrepository.command.createRepoFromModel"
            name="Add Model to Workspace">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.CloneModelHandler"
            id="com.archimatetool.modelrepository.command.cloneModel"
            name="Import Remote Model to Workspace">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.RefreshModelHandler"
            id="com.archimatetool.modelrepository.command.refreshModel"
            name="Refresh Model">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.CommitModelHandler"
            id="com.archimatetool.modelrepository.command.commitModel"
            name="Commit Changes">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.PushModelHandler"
            id="com.archimatetool.modelrepository.command.pushModel"
            name="Publish Changes">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.DiscardChangesHandler"
            id="com.archimatetool.modelrepository.command.discardChanges"
            name="Discard Uncommitted Changes">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            defaultHandler="com.archimatetool.modelrepository.handlers.DeleteModelHandler"
            id="com.archimatetool.modelrepository.command.deleteModel"
            name="Delete Model from Workspace">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            id="com.archimatetool.modelrepository.newGroup"
            name="New Group">
      </command>
      <command
            categoryId="com.archimatetool.modelrepository.category"
            id="com.archimatetool.modelrepository.addExistingRepository"
            name="Add Existing Repository">
      </command>
   </extension>
