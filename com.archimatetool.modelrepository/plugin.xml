            class="com.archimatetool.modelrepository.preferences.ModelRepositoryPreferencePage"
            id="com.archimatetool.com.modelrepository.preferences.ModelRepositoryPreferencePage"
            name="Collaboration">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="com.archimatetool.editor">
         <propertyCategory
               category="com.archimatetool.modelrepository.category"></propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="com.archimatetool.editor">
         <propertyTab
               category="com.archimatetool.modelrepository.category"
               id="repo.tab"
               label="Repository">
         </propertyTab>
         <propertyTab
               afterTab="info.tab"
               category="com.archimatetool.modelrepository.category"
               id="user.tab"
               label="User Details">
         </propertyTab>
         <propertyTab
               afterTab="user.tab"
               category="com.archimatetool.modelrepository.category"
               id="auth.tab"
               label="Authentication">
         </propertyTab>
         <propertyTab
               category="com.archimatetool.modelrepository.category"
               id="group.tab"
               label="Group">
         </propertyTab>
      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="com.archimatetool.editor">
         <propertySection
               class="com.archimatetool.modelrepository.propertysections.RepoInfoSection"
               filter="com.archimatetool.modelrepository.propertysections.RepoInfoSection$Filter"
               id="infoSection"
               tab="repo.tab">
         </propertySection>
         <propertySection
               class="com.archimatetool.modelrepository.propertysections.UserDetailsSection"
               filter="com.archimatetool.modelrepository.propertysections.UserDetailsSection$Filter"
               id="userSection"
               tab="user.tab">
         </propertySection>
         <propertySection
               class="com.archimatetool.modelrepository.propertysections.AuthSection"
               filter="com.archimatetool.modelrepository.propertysections.AuthSection$Filter"
               id="authSection"
               tab="auth.tab">
         </propertySection>
         <propertySection
               class="com.archimatetool.modelrepository.propertysections.GroupSection"
               filter="com.archimatetool.modelrepository.propertysections.GroupSection$Filter"
               id="groupSection"
               tab="group.tab">
         </propertySection>
      </propertySections>
   </extension>
   <extension
